;; * Code

(go-lspa
 :root "go-lspa"
 :prebuilt
 ((gnu/linux
   ((x86_64 :init "eemacs-lspa-go-lspa-PreBuilt-GnuLinux-x86_64-loader.el")))
  (windows-nt
   ((x86_64 :init "eemacs-lspa-go-lspa-PreBuilt-WindowsNT-x86_64-loader.el")))))
